{
  "name": "paralleljs",
  "version": "0.2.1",
  "description": "parallel.js enables easy multi-thread processing in javascript",
  "author": "Adam Savitzky <adam.savitzky@gmail.com>",
  "contributors": [
    "Sebastian Mayr <sebmaster16@gmail.com> (http://s3bmaster.blogspot.co.at/)",
    "Amila Welihinda <amilajack@gmail.com> (https://amilajack.com/)"
  ],
  "license": "BSD",
  "scripts": {
    "lint": "eslint lib",
    "test": "npm run lint && jasmine-node --verbose test/specs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adambom/parallel.js.git"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "parallel",
    "spawn",
    "map",
    "thread",
    "parallel.js",
    "workers",
    "webworkers"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-bliss": "^4.7.0",
    "jasmine-node": "x",
    "q": "x"
  },
  "browser": {
    "child_process": false
  },
  "engines": {
    "node": ">=10"
  },
  "testling": {
    "scripts": [
      "lib/parallel.js",
      "test/jasmine/jasmine.js",
      "test/jasmine/jasmine.tap_reporter.js",
      "test/specs/*.js",
      "test/runner.js"
    ],
    "browsers": [
      "ie/9..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "bliss",
    "env": {
      "jest": false,
      "jasmine": true,
      "browser": true
    }
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
